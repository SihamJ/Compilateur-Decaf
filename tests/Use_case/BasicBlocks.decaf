class BasicBlocks {


    void main(){
        int a, b, c, d, e, f, g, h;

        a = 10;
        b = 5;
        c = 2;
        d = 1;

        if(a*b + c > 30){
            a = a + 2;
            b = b * a;
            c += a;
            d += c;
            if( d > 5) {
                e = 2 * a;
                f = e * a + c;
                g = e + f;
                h = g * 3 + g;
            }
        }
        else {
            a = a - 2;
            b = b + a;
            c = a;
            d = a;
            e = 2 * b;
            f = e * a;
            g = e + f;
            h = g * 3;
        }
    }
}